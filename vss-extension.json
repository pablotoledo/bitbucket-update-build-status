{
    "manifestVersion": 1,
    "id": "bitbucket-commit-status",
    "name": "Bitbucket Server Commit Status",
    "version": "0.1.0",
    "publisher": "pablotoledo",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "This plugin allow you to send the status of your build to Bitbucket",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icono.png"        
    },
    "files": [
        {
            "path": "bbplugin"
        }
    ],
    "contributions": [
        {
            "id": "bitbucket-commit-status-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "bbplugin"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/pablotoledo/bitbucket-update-build-status/"
    },
    "content": {
        "details": {
            "path": "readme.md"
        }
    }
}